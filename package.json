{
  "name": "@marklundin/oui",
  "version": "0.0.1",
  "description": "A stupidly simple way of instrumenting code and making user interfaces",
  "main": "lib",
  "scripts": {
    "compile:npm": "babel src --out-dir lib",
    "compile:prod": "NODE_ENV=production browserify --standalone oui -t babelify -g aliasify -g envify -g uglifyify src/index.js | uglifyjs -m > dist/oui.min.js",
    "compile:dev": "NODE_ENV=production browserify -d --standalone oui -t babelify -g aliasify -g envify -g uglifyify src/index.js > dist/oui.js",
    "start": "NODE_ENV=development budo index.js --live -- -t babelify -g aliasify",
    "test": "ava tests -v"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marklundin/oui.git"
  },
  "keywords": [
    "gui",
    "demos"
  ],
  "author": "Mark Lundin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marklundin/oui/issues"
  },
  "homepage": "https://github.com/marklundin/oui#readme",
  "ava": {
    "require": [
      "./tests/helpers/setup-browser-env.js",
      "babel-register"
    ]
  },
  "dependencies": {
    "core-controllers": "github:marklundin/core-controllers",
    "es7-reflect-metadata": "^1.6.0",
    "fast.js": "^0.1.1",
    "react": "^15.0.1",
    "react-addons-shallow-compare": "^15.0.1",
    "react-dom": "^15.0.1"
  },
  "devDependencies": {
    "aliasify": "^2.0.0",
    "ava": "^0.14.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babelify": "^7.2.0",
    "budo": "^8.2.1",
    "envify": "^3.4.0",
    "jsdom": "^8.4.0",
    "uglify-js": "^2.6.2",
    "uglifyify": "^3.0.1"
  }
}
