{
  "name": "@marklundin/oui",
  "version": "0.0.1",
  "description": "A stupidly simple way of instrumenting code and making user interfaces",
  "main": "lib",
  "scripts": {
    "compile:npm": "babel src --out-dir lib",
    "compile:prod": "NODE_ENV=production browserify --standalone oui -t babelify -g aliasify -g envify -g uglifyify src/index.js | uglifyjs -m > dist/oui.min.js",
    "compile:dev": "NODE_ENV=development browserify -d --standalone oui -t babelify -g aliasify -g envify -g uglifyify src/index.js > dist/oui.js",
    "start": "NODE_ENV=development budo index.js --live -- -t babelify -g aliasify",
    "test": "karma start build/karma.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marklundin/oui.git"
  },
  "keywords": [
    "gui",
    "demos"
  ],
  "author": "Mark Lundin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marklundin/oui/issues"
  },
  "homepage": "https://github.com/marklundin/oui#readme",
  "ava": {
    "require": [
      "babel-register",
      "./tests/helpers/setup-browser-env.js"
    ],
    "babel": "inherit"
  },
  "aliasify": {
    "aliases": {
      "react": "preact-compat",
      "react-dom": "preact-compat",
      "react-addons-shallow-compare" : "preact-shallow-compare"
    }
  },
  "dependencies": {
    "es7-reflect-metadata": "^1.6.0",
    "fast.js": "^0.1.1",
    "preact-compat": "^1.7.1",
    "preact-shallow-compare": "^1.0.1",
    "react": "^15.0.1",
    "react-addons-shallow-compare": "^15.0.1",
    "react-dom": "^15.0.1"
  },
  "devDependencies": {
    "aliasify": "^2.0.0",
    "babel-preset-airbnb": "^2.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "budo": "^8.2.1",
    "envify": "^3.4.0",
    "enzyme": "^2.3.0",
    "jasmine-core": "^2.4.1",
    "jasmine-enzyme": "^1.0.0",
    "jsdom": "^8.4.0",
    "karma": "^0.13.22",
    "karma-browserify": "^5.0.5",
    "karma-chrome-launcher": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "sinon": "^1.17.4",
    "uglify-js": "^2.6.2",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0"
  }
}
