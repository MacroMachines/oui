{
  "name": "@marklundin/oui",
  "version": "0.0.1",
  "description": "A stupidly simple way of instrumenting code and making user interfaces",
  "main": "lib",
  "scripts": {
    "build": "node build/build.js",
    "compile:npm": "babel src --out-dir lib",
    "compile:prod": "NODE_ENV=production browserify --standalone oui -t babelify -g aliasify -g envify -g uglifyify src/index.js | uglifyjs -m > dist/oui.min.js",
    "compile:dev": "NODE_ENV=development browserify -d --standalone oui -t babelify -g aliasify -g envify -g uglifyify src/index.js > dist/oui.js",
    "start": "NODE_ENV=development budo index.js --live -- -t babelify -g aliasify",
    "test": "NODE_ENV=production karma start build/karma.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marklundin/oui.git"
  },
  "keywords": [
    "gui",
    "demos"
  ],
  "author": "Mark Lundin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marklundin/oui/issues"
  },
  "homepage": "https://github.com/marklundin/oui#readme",
  "dependencies": {
    "babel-core": "^6.9.0",
    "es7-reflect-metadata": "^1.6.0",
    "preact-compat": "^1.7.1",
    "preact-shallow-compare": "^1.0.1",
    "react": "^15.1.0",
    "react-addons-shallow-compare": "^15.1.0",
    "react-dom": "^15.1.0"
  },
  "devDependencies": {
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-airbnb": "^2.0.0",
    "babel-preset-react": "^6.5.0",
    "enzyme": "^2.3.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-webpack": "^1.7.0",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "webpack": "^1.13.1"
  }
}
