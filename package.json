{
  "name": "dui",
  "version": "0.0.1",
  "description": "A prototyping library to create UI for instrumenting code",
  "main": "build/dui.min.js",
  "scripts": {
    "build": "npm run production && npm run dev",
    "production": "NODE_ENV=production browserify --standalone dui -t babelify -g envify -g uglifyify src/index.js | uglifyjs -m > build/dui.min.js",
    "dev": "NODE_ENV=production browserify -d --standalone dui -t babelify -g envify -g uglifyify src/index.js > build/dui.js",
    "start": "NODE_ENV=development budo index.js --live -- -t babelify",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marklundin/dui.git"
  },
  "keywords": [
    "gui",
    "demos"
  ],
  "author": "Mark Lundin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marklundin/dui/issues"
  },
  "homepage": "https://github.com/marklundin/dui#readme",
  "dependencies": {
    "core-controllers": "github:marklundin/core-controllers",
    "deepmerge": "^0.2.10",
    "es7-reflect-metadata": "^1.6.0",
    "react": "^15.0.1",
    "react-addons-shallow-compare": "^15.0.1",
    "react-dom": "^15.0.1",
    "reflect-metadata": "^0.1.3"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babelify": "^7.2.0",
    "budo": "^8.2.1",
    "envify": "^3.4.0",
    "uglify-js": "^2.6.2",
    "uglifyify": "^3.0.1"
  }
}
